{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://sdf-pt10-group-09.onrender.com/api/v1/$1"
    },
    {
      "src": "/ajali.svg",
      "dest": "/ajali.svg"
    },
    {
      "src": "/favicon.ico",
      "dest": "/ajali.svg"  // Redirects favicon.ico request to ajali.svg
    },
    {
      "src": "/((?!api|_next/static|_next/image|favicon.ico|ajali.svg).*)",
      "dest": "/index.html"
    }
  ]
}